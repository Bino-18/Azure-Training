{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable3",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Project1_raw",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          OrderID as string,",
				"          CustomerName as string,",
				"          Category as string,",
				"          SubCategory as string,",
				"          City as string,",
				"          OrderDate as date,",
				"          Region as string,",
				"          Sales as decimal(10,2),",
				"          Discount as decimal(5,2),",
				"          Profit as decimal(10,2),",
				"          State as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 aggregate(groupBy(Category,",
				"          SubCategory),",
				"     Sales = sum(Sales)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Order ID} as string,",
				"          {Customer Name} as string,",
				"          Category as string,",
				"          {Sub Category} as string,",
				"          City as string,",
				"          {Order Date} as string,",
				"          Region as string,",
				"          Sales as string,",
				"          Discount as string,",
				"          Profit as string,",
				"          State as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}